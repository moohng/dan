{
  "name": "@moohng/dan",
  "version": "1.3.9",
  "description": "js工具库",
  "main": "lib/dan.js",
  "module": "src/index.js",
  "browser": "dist/dan.min.js",
  "scripts": {
    "dev": "rollup -c config/rollup.config.dev.js -w --server",
    "watch": "rollup -c config/rollup.config.dev.js -w",
    "build": "rollup -c config/rollup.config.dev.js",
    "build:mini": "rollup -c config/rollup.config.prod.js",
    "lint": "eslint --ext .js src --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "util",
    "utils"
  ],
  "files": [
    "lib/",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:moohng/dan.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kevin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.0.0"
  }
}
