{
  "name": "@moohng/dan",
  "version": "1.3.19",
  "description": "js工具库",
  "main": "lib/dan.js",
  "module": "src/index.js",
  "browser": "dist/dan.min.js",
  "scripts": {
    "dev": "rollup -c config/rollup.config.dev.js -w --server",
    "watch": "rollup -c config/rollup.config.dev.js -w",
    "build": "rollup -c config/rollup.config.dev.js",
    "build:mini": "rollup -c config/rollup.config.prod.js",
    "lint": "eslint --ext .js src --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "util",
    "utils"
  ],
  "files": [
    "lib/",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:moohng/dan.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kevin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "lint-staged": "^10.1.7",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.3.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
